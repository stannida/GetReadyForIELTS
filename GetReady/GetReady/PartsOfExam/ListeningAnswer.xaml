        <Rectangle Fill="#FF2DB458" HorizontalAlignment="Center" Height="60" Stroke="#00000000" VerticalAlignment="Top" Width="594" IsHitTestVisible="False" Margin="0,0,1,0" Grid.ColumnSpan="2"/>
