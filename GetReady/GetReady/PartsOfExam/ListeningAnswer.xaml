        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ListeningAnswer" Height="400" Width="600">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="298*"/>
            <ColumnDefinition Width="297*"/>
