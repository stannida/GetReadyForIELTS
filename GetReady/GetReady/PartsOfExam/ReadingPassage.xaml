<Window x:Class="GetReady.PartsOfExam.ReadingPassage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GetReady.PartsOfExam"
        mc:Ignorable="d"
        Title="ReadingPassage" Height="700" Width="700">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="headline" HorizontalAlignment="Left" Margin="98,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="16" Width="498" Grid.ColumnSpan="3" FontFamily="Levenim MT" FontWeight="Bold" TextAlignment="Center"/>
        <ScrollViewer Grid.ColumnSpan="3" Margin="10,43,10,10">
            <TextBlock x:Name="passage" HorizontalAlignment="Left" ScrollViewer.VerticalScrollBarVisibility="Visible" ScrollViewer.CanContentScroll="True" TextWrapping="Wrap" VerticalAlignment="Top" Grid.ColumnSpan="3" Height="1031" Width="644" FontFamily="Levenim MT" FontSize="10.667" TextAlignment="Justify" />
        </ScrollViewer>
    </Grid>
</Window>
